{
    "_base": "basic@pretrained_ner",
    "config": {
        /*"tokenizer_config_path": "./local_data/embeddings/embedding_tokenizer.json", // the tokenizer config path (the tokenizer.json path)*/
        "feed_order": ["load", "ner_prepro", "tokenizer", "ner_firstpiece_relabel", "token_gather", "label_to_id", "save"]
