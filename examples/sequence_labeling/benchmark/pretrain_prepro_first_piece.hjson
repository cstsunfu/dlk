        "tokenizer_config_path": "local_data/pretrained_model/bert_case/indent_tokenizer.json", // the tokenizer config path (the tokenizer.json path)
        "data_dir": "output/benchmark/pretrained/",  // save load data base dir
    },
    "subprocessor@ner_firstpiece_relabel":{
        "_base": "ner_firstpiece_relabel",
        "config": {
            "train":{ //train、predict、online stage config,  using '&' split all stages
