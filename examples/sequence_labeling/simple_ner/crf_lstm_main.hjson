{
    "root": {
        "_name": "test_cls",
        "_link": {"config.meta_data": ['task.imodel.model.config.embedding_file', 'task.imodel.postprocessor.config.meta']},
        "_search": {},
        "config": {
            "save_dir": "test_simple_ner",  # must provide
            "data_path": "test_simple_ner/processed_data.pkl",  # must provide
            "meta_data": "test_simple_ner/meta.pkl",  # must provide
        },
        "task": {
            "_name": "test_token_cls",
            "manager": {
                "_base": "lightning",
                "config":{
                    "max_epochs": 10,
                }
            },
            "imodel": {
                "_base": "basic@crf_sequence_labeling",
                "model": {
                    "_base": "basic@lstmcrf4sequencelabeling",
                    "config": {
                        "embedding_dim": 200,
                        "dropout": 0.1,
                        "embedding_file": "*@*",
                        "label_num": 11,
                    },
                },
                "optimizer": {
                    "config": {
                        "lr": 1e-3,
                    },
                },
                "postprocessor": {
                    "config": {
                        "meta": '*@*'
                    }
                },
            },
            "datamodule": {
                "_base": "basic@test_token_cls",
            },
        },
    },
    "_focus": {
        "root._name": "task."
    },
}
